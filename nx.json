{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default",
      "!{projectRoot}/eslint.config.cjs"
    ],
    "sharedGlobals": [
      "{workspaceRoot}/azure-pipelines.yml"
    ]
  },
  "nxCloudId": "67924194d5bb817417957cfd",
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    }
  ]
}
